[DEFAULT]
git_gc = git gc "$@"
jobs = 5

include = cat ~/.config/mr/config.d/*

[.config/nvim/bundle/neobundle]
checkout = git clone 'https://github.com/Shougo/neobundle.vim.git' 'neobundle'
